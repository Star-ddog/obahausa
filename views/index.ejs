<section id="testimonial">
  <div class="container px-5 mx-auto text-center md:max-w-6xl">
          <!-- Testimonial continer -->
      <div class="flex flex-col space-y-16 mt-24 md:flex-row md:space-y-0 md:space-x-6">
         
